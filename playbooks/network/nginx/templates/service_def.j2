apiVersion: v1
kind: Service
metadata:
  name: '{{ nginx_svc.metadata.name }}'
  namespace: '{{ nginx_svc.metadata.namespace }}'
  resourceVersion: '{{ nginx_svc.metadata.resourceVersion }}'
spec:
  ports:
{% for aport in tobeadded %}
  - name: tcp{{ aport.key }}
    port: {{ aport.key|int }}
    protocol: TCP
    targetPort: {{ aport.key|int }}
{%  endfor %}